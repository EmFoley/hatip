<div id="my_image">
  <%= image_tag "guitar.png", class: "main-image" %>
<button id="button_id" class="main_image_button">lala</button>
</div>


<div class="card-container">
  <div id="card-wrapper">
    <span class="filters">
      <span class="filter"><img src="https://cdn1.iconfinder.com/data/icons/metro-uinvert-dock/128/Music_Library.png" height="16" /> <%= link_to "Musical", show_music_path, remote: true, class: "filter_music" %> </span>
      <span class="filter"><img src="https://cdn1.iconfinder.com/data/icons/windows8_icons_iconpharm/26/yoga.png" height="16"> <%= link_to "Dance", show_dance_path, remote: true, class: "filter_dance" %> </span>
      <span class="filter"><img src="https://cdn1.iconfinder.com/data/icons/customicondesign-office7-shadow-png/128/Sport-hurdle.png" height="16" /> <%= link_to "Acrobatic", show_acrobatic_path, remote: true, class: "filter_acrobatic" %> </span>
      <span class="filter"><img src="https://cdn1.iconfinder.com/data/icons/super-mono-basic/yellow/medal_basic_yellow.png" height="16" /> <%= link_to "Other", show_other_path, remote: true, class: "filter_other" %></span>
    </span>
    <br/><br/>
    <div class="home_cards">
      <%= render partial: 'pages/post_cards', locals: { posts: @posts } %>
    </div>
  </div>
  <%= render partial: 'pages/post_card_modal' %>
</div>



