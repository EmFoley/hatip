<div class="container">
<h1>Welcome to <%= @user.stage_name %>'s Profile</h1>
<iframe src="//www.facebook.com/plugins/like.php?href=http%3A%2F%2Fguarded-journey-5941.herokuapp.com%2Fusers%2F<%=@user.id%>&amp;width&amp;layout=button_count&amp;action=like&amp;show_faces=true&amp;share=true&amp;height=21&amp;appId=548149488617364" scrolling="no" frameborder="0" style="border:none; overflow:hidden; height:21px;" allowTransparency="true"></iframe>
  <% if may_edit_profile %>
    <div>
      <%= link_to "Edit Profile", edit_profile_path(@user) %>
    </div>
  <% end %>

  <div class='tv'>
    <%= raw( @user.highlight_youtube_url) if @user.featured_youtube_url %>
  </div>

  <button>Tip the Hat!</button>
  <%# if may_edit_profile %>
    <button id="new_post_button">Create New Post</button>
    <%= render partial: 'posts/post_form', post: @post %>
  <%# end %>

  <%= render partial: 'profile_content' %>
  <%= render partial: 'posts/artist_posts' %>
</div>